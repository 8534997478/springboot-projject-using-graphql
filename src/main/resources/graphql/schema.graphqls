type Mutation {
    createBook(book: BookInput): Book
}

type Query {
    getALL: [Book]
    bookgetbyId(id: Int): Book
}

type Book {
    id: ID!
    title: String
    description: String
    author: String
    price: Float
    pages: Int
}

input BookInput {
    title: String
    description: String
    author: String
    price: Float
    pages: Int
}
